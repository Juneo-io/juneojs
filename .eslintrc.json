{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.eslint.json",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/no-non-null-assertion": "off",
    "@typescript-eslint/no-unsafe-argument": "warn"
  }
}
